                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/****** Script for SelectTopNRows command from SSMS  ******/
SELECT 
		cast ([ResourceUID] as   uniqueidentifier )  as  [ResourceUID]
      , cast ( [ResourceName] as   [varchar](100) )  as  [ResourceName]
     
      , cast ( [ResourceEmailAddress] as  [varchar](100)  )  as  [ResourceEmailAddress]
	  , cast ( right([ResourceNTAccount], len([ResourceNTAccount]) - charindex('|', [ResourceNTAccount])) as varchar(20)) as [ResourceNTAccount]
     
      , cast ( [ResourceIsActive] as    bit )  as [ResourceIsActive]
     
      , cast ( [ResourceCount] as  int )  as [ResourceCount]
      , cast ( [RBS] as   [varchar](4000) )  as [RBS]
     
      , cast ( [Resource Role] as  [varchar](50)  )  as [Resource Role]
      , cast ( [Time Reporting Exempt] as   [varchar](10) )  as [Time Reporting Exempt]
      , cast ( [Home Location US] as   [varchar](12) )  as [Home Location US]
      , cast ( [Department Number] as  [varchar](12)  )  as [Department Number]
      , cast ( [Employee Type] as  [varchar](55)  )  as [Employee Type]
      , cast ( [Resource Title] as   [varchar](55) )  as [Resource Title] 
	  
	
     
  FROM [ProjectWebApp].[dbo].[MSP_EpmResource_UserView] 
  where [ResourceIsGeneric] = 0</property>
