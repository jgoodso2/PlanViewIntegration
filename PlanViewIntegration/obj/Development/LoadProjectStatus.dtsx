                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH DateFields AS
	(
		SELECT	GETDATE() AS [Today]
			  , DATEADD(DD,1,GETDATE()) AS [TodayPlus1]
			  , DATEADD(DD,5,GETDATE()) AS [TodayPlus5]
			  , DATEADD(DD,15,GETDATE()) AS [TodayPlus15]
			  , DATEADD(DD,30,GETDATE()) AS [TodayPlus30]
			  , DATEADD(DD,45,GETDATE()) AS [TodayPlus45]
			  , DATEADD(DD,60,GETDATE()) AS [TodayPlus60]
			  , DATEADD(DD,75,GETDATE()) AS [TodayPlus75]
			  , DATEADD(DD,90,GETDATE()) AS [TodayPlus90]
			  , DATEADD(DD,-5,GETDATE()) AS [TodayMinus5]
			  , DATEADD(DD,-15,GETDATE()) AS [TodayMinus15]
			  , DATEADD(DD,-30,GETDATE()) AS [TodayMinus30]
			  , DATEADD(DD,-45,GETDATE()) AS [TodayMinus45]
			  , DATEADD(DD,-60,GETDATE()) AS [TodayMinus60]
			  , DATEADD(DD,-75,GETDATE()) AS [TodayMinus75]
			  , DATEADD(DD,-90,GETDATE()) AS [TodayMinus90]
	)
	
	
	SELECT 
		    cast (TUV.[TaskUID] as [uniqueidentifier]  ) as [TaskUID]
,cast ( sys.fn_varbintohexsubstring(0, HashBytes('SHA1', CAST(TUV.taskuid AS char(40))), 1, 0) as char(42))  as tuid
,  cast (   TUV.[TaskParentUID] as [uniqueidentifier]  ) as    [TaskParentUID]
,  cast (   TUV.[ProjectUID] as [uniqueidentifier]  ) as    [ProjectUID]
,  cast (   TUV.[TaskName] as [varchar](255)  ) as    [TaskName]
,  cast (   TUV.[TaskIndex] as int ) as    [TaskIndex]
,  cast (   TUV.[TaskIsOverallocated] as varchar(3) ) as    [TaskIsOverallocated]
,  cast (   TUV.[TaskIsMilestone] as varchar(3) ) as   [TaskIsMilestone]
,  cast (   TUV.[TaskIsCritical] as varchar(3) ) as   [TaskIsCritical]
,  cast (   TUV.[TaskIsSummary] as varchar(3) ) as  [TaskIsSummary]

,  cast (   TUV.[TaskDeliverableStartDate] as date ) as  [TaskDeliverableStartDate]
,  cast (   TUV.[TaskDeliverableFinishDate] as date ) as  [TaskDeliverableFinishDate]

,  cast (   TUV.[TaskDurationVariance] / 8 as decimal (25,2)) as  [TaskDurationVariance]
,  cast (   TUV.[TaskStartVariance] as decimal (25,2) ) as [TaskStartVariance]
,  cast (   TUV.[TaskFinishVariance] as decimal (25,2) ) as [TaskFinishVariance]
		  , cast ( TUV.[TaskDuration] / 8 as decimal (25,2) ) AS [TaskDuration]
		  , CAST(TUV.[TaskStartDate] AS DATE) AS [TaskStartDate] 
		  , CAST(TUV.[TaskFinishDate] AS DATE) AS [TaskFinishDate]
		  , cast ( TUV.[TaskPercentCompleted] as int) as [TaskPercentCompleted]
		
		  , cast ( TUV.[TaskIsActive] as varchar(3)) as TaskIsActive
		  , cast ( TUV.[TaskIsManuallyScheduled] as varchar(3)) as taskIsManuallyScheduled
		  , CAST(TUV.[TaskCreatedDate] AS DATE) AS [TaskCreatedDate]
		  , CAST(TUV.[TaskModifiedDate] AS DATE) AS [TaskModifiedDate]
		  , cast ( TUV.[TaskCount] as int) as taskCount
		  --, TUV.[WBS_]
		  , cast ( TUV.TaskPhysicalPercentCompleted as int) as TaskPhysicalPercentCompleted
		  , CAST(TUV.TaskBaseline0StartDate AS DATE) AS TaskBaseline0StartDate
		  , CAST(TUV.TaskBaseline0FinishDate AS DATE) AS TaskBaseline0FinishDate
	
		  , cast (TUV.[Report Grouping] as varchar(50))  as Report_key_text
		  , cast ( TUV.[Report Grouping] as varchar(50))  as [Report_Grouping_Text]
		  , cast ( TUV.[Task Comments] as varchar(500) ) as [Task Comments]
		  ,CAST(TUV.TaskActualFinishDate AS DATE) AS TaskActualFinishDate

		  , CAST(TUV.TaskWBS AS varchar(50)) AS WBS
		  , CAST(TUV.TaskActualStartDate AS DATE) AS TaskActualStartDate

		  , cast ( TUV.TaskBaseline0Duration / 8 AS decimal (25,2)) as TaskBaseline0Duration
		  , cast ( TUV.[Key Task] as varchar(3)) as [Key Task]
		  , cast ( TUV.[Deliverable Flag] as varchar(3)) as [Deliverable Flag]
		  , cast ( TUV.[Milestone Flag] as varchar(3)) as   [Milestone Flag]

		  -- 01/16/2015 - Added ActualFinishDateRange field to View
		  , cast ( CASE WHEN TUV.TaskActualFinishDate IS NULL THEN NULL ELSE
				CASE WHEN TUV.TaskActualFinishDate BETWEEN DF.[TodayMinus5] AND DF.[Today] THEN 'Finished in Past 5 Days' 
					 WHEN TUV.TaskActualFinishDate BETWEEN DF.[TodayMinus15] AND DF.[Today] THEN 'Finished in Past 15 Days' 
					 WHEN TUV.TaskActualFinishDate BETWEEN DF.[TodayMinus30] AND DF.[Today] THEN 'Finished in Past 30 Days' 
					 WHEN TUV.TaskActualFinishDate BETWEEN DF.[TodayMinus45] AND DF.[Today] THEN 'Finished in Past 45 Days' 
					 WHEN TUV.TaskActualFinishDate BETWEEN DF.[TodayMinus60] AND DF.[Today] THEN 'Finished in Past 60 Days' 
					 WHEN TUV.TaskActualFinishDate BETWEEN DF.[TodayMinus75] AND DF.[Today] THEN 'Finished in Past 75 Days' 
					 WHEN TUV.TaskActualFinishDate BETWEEN DF.[TodayMinus90] AND DF.[Today] THEN 'Finished in Past 90 Days' 
		   		END
			END  as varchar(100) ) as [ActualFinishDateRange]
		  , cast (  TUV.[Responsible Party] as varchar(50)) as [Responsible Party]
		  , cast ( CASE WHEN TUV.TaskActualFinishDate IS NULL THEN
				CASE WHEN TUV.TaskFinishDate BETWEEN DF.[Today] AND DF.[TodayPlus5] THEN 'Forecast Finish Next 5 Days' 
					 WHEN TUV.TaskFinishDate BETWEEN DF.[Today] AND DF.[TodayPlus15] THEN 'Forecast Finish Next 15 Days' 
					 WHEN TUV.TaskFinishDate BETWEEN DF.[Today] AND DF.[TodayPlus30] THEN 'Forecast Finish Next 30 Days' 
					 WHEN TUV.TaskFinishDate BETWEEN DF.[Today] AND DF.[TodayPlus45] THEN 'Forecast Finish Next 45 Days' 
					 WHEN TUV.TaskFinishDate BETWEEN DF.[Today] AND DF.[TodayPlus60] THEN 'Forecast Finish Next 60 Days' 
					 WHEN TUV.TaskFinishDate BETWEEN DF.[Today] AND DF.[TodayPlus75] THEN 'Forecast Finish Next 75 Days' 
					 WHEN TUV.TaskFinishDate BETWEEN DF.[Today] AND DF.[TodayPlus90] THEN 'Forecast Finish Next 90 Days' 
				END
				ELSE NULL
			END as varchar(100)) AS [ForecastToFinish]
		  -- 01/16/2015 - Added ForecastToStart to View
		  , cast ( CASE WHEN TUV.TaskActualStartDate IS NULL THEN
				CASE WHEN TUV.TaskStartDate BETWEEN DF.[Today] AND DF.[TodayPlus5] THEN 'Forecast Start Next 5 Days' 
					 WHEN TUV.TaskStartDate BETWEEN DF.[Today] AND DF.[TodayPlus15] THEN 'Forecast Start Next 15 Days' 
					 WHEN TUV.TaskStartDate BETWEEN DF.[Today] AND DF.[TodayPlus30] THEN 'Forecast Start Next 30 Days' 
					 WHEN TUV.TaskStartDate BETWEEN DF.[Today] AND DF.[TodayPlus45] THEN 'Forecast Start Next 45 Days' 
					 WHEN TUV.TaskStartDate BETWEEN DF.[Today] AND DF.[TodayPlus60] THEN 'Forecast Start Next 60 Days' 
					 WHEN TUV.TaskStartDate BETWEEN DF.[Today] AND DF.[TodayPlus75] THEN 'Forecast Start Next 75 Days' 
					 WHEN TUV.TaskStartDate BETWEEN DF.[Today] AND DF.[TodayPlus90] THEN 'Forecast Start Next 90 Days' 
				END
				ELSE NULL
			END as varchar(100)) AS [ForecastToStart]
			-- 01/16/2015 - Added BaselinedToFinish to View
		  , cast ( CASE WHEN TUV.TaskBaseline0FinishDate IS NOT NULL THEN
				CASE WHEN TUV.TaskBaseline0FinishDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus5] THEN 'Baselined To Finish Next 5 days' 
					 WHEN TUV.TaskBaseline0FinishDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus15] THEN 'Baselined To Finish Next 15 days' 
					 WHEN TUV.TaskBaseline0FinishDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus30] THEN 'Baselined To Finish Next 30 days' 
					 WHEN TUV.TaskBaseline0FinishDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus45] THEN 'Baselined To Finish Next 45 days' 
					 WHEN TUV.TaskBaseline0FinishDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus60] THEN 'Baselined To Finish Next 60 days' 
					 WHEN TUV.TaskBaseline0FinishDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus75] THEN 'Baselined To Finish Next 75 days' 
					 WHEN TUV.TaskBaseline0FinishDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus90] THEN 'Baselined To Finish Next 90 days' 
				END
				ELSE NULL
			END as varchar(100)) AS [BaselinedToFinish]
		  -- 01/16/2015 - Added BaselinedToStart to View
		  , cast ( CASE WHEN TUV.TaskBaseline0StartDate IS NOT NULL THEN
				CASE WHEN TUV.TaskBaseline0StartDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus5] THEN 'Baselined To Start Next 5 days' 
					 WHEN TUV.TaskBaseline0StartDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus15] THEN 'Baselined To Start Next 15 days' 
					 WHEN TUV.TaskBaseline0StartDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus30] THEN 'Baselined To Start Next 30 days' 
					 WHEN TUV.TaskBaseline0StartDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus45] THEN 'Baselined To Start Next 45 days'
					 WHEN TUV.TaskBaseline0StartDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus60] THEN 'Baselined To Start Next 60 days'
					 WHEN TUV.TaskBaseline0StartDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus75] THEN 'Baselined To Start Next 75 days' 
					 WHEN TUV.TaskBaseline0StartDate BETWEEN DF.[TodayPlus1] AND DF.[TodayPlus90] THEN 'Baselined To Start Next 90 days' 
				END
				ELSE NULL
			END as varchar(100)) as [BaselinedToStart]
		  , CAST(CASE WHEN (TUV.[TaskDuration] = 0 OR TUV.[Milestone Flag] = 'Yes' OR TUV.[TaskIsMilestone] = 1) 
									AND TUV.TaskBaseline0FinishDate &lt;= DF.[Today] AND TUV.TaskIsActive = 1 
					THEN 1 ELSE 0 
			END AS varchar(3)) AS ISScheduledMilestone
		  --, CAST(CASE WHEN TUV.TaskIsMilestone = 1 AND TUV.TaskBaseline0FinishDate &lt;= DF.[Today] AND TUV.TaskActualFinishDate IS NOT NULL 
		  , CAST(CASE WHEN (TUV.[TaskDuration] = 0 OR TUV.[Milestone Flag] = 'Yes' OR TUV.[TaskIsMilestone] = 1) 
								AND TUV.TaskBaseline0FinishDate &lt;= DF.[Today] AND TUV.TaskActualFinishDate IS NOT NULL 
				THEN 1 ELSE 0 
			END AS varchar(3)) AS ISCompletedMilestone
		  -- 02/10/2015 - Added TaskIsScheduledDeliverable and TaskIsCompletedDeliverable to view
		  , CAST(CASE WHEN (TUV.[TaskDeliverableStartDate] IS NOT NULL OR TUV.[Deliverable Flag] = 'Yes')
					AND TUV.TaskBaseline0FinishDate &lt;= DF.[Today] THEN 1 ELSE 0
			END AS varchar(3)) AS ISScheduledDeliverable
		  , CAST(CASE WHEN (TUV.[TaskDeliverableStartDate] IS NOT NULL OR TUV.[Deliverable Flag] = 'Yes')
					AND TUV.TaskActualFinishDate IS NOT NULL THEN 1 ELSE 0
			END AS varchar(3)) AS ISCompletedDeliverable
		  -- 02/11/2015 - Added ISIncompletedTask and ISBaselinedToFinish to view
		  , CAST(CASE WHEN TUV.TaskBaseline0FinishDate IS NOT NULL AND TUV.TaskBaseline0FinishDate &lt;= DF.[Today] 
					AND TUV.TaskActualFinishDate IS NULL THEN 1 ELSE 0 
			END AS varchar(3)) AS ISIncompletedTask -- numerator
		  , CAST(CASE WHEN TUV.TaskBaseline0FinishDate IS NOT NULL AND TUV.TaskBaseline0FinishDate &lt;= DF.[Today] THEN 1 ELSE 0 
			END AS varchar(3)) AS ISBaselinedToFinish -- denominator
		  , CAST(CASE WHEN TUV.TaskActualFinishDate IS NOT NULL AND TUV.TaskBaseline0FinishDate IS NOT NULL THEN 1 ELSE 0 END AS varchar(3)) AS TaskIsBaselinedAndComplete




	FROM	ProjectWebApp.dbo.[MSP_EpmTask_UserView] TUV WITH (NOLOCK)
			--LEFT JOIN ProjectWebApp.dbo.MSP_EpmTask_UserView TUV WITH (NOLOCK) 
			--	ON Tov.TaskUID = TOV.TaskUID
			LEFT JOIN ProjectWebApp.dbo.MSP_EpmProject_UserView PUV 
				ON PUV.ProjectUID = TUV.ProjectUID
				
			-- 08/01/2016 - Removed Num of Invalid Characters Logic from View
			--LEFT JOIN temp_TaskData td
			--	ON TUV.TaskUID = td.TaskUID
			CROSS JOIN DateFields DF 

			where puv.[Project Active Status] = 'In Progress'</property>
