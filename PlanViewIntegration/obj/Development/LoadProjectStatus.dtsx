                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select  
cast(sys.fn_varbintohexsubstring(0, HashBytes('SHA1', CAST(ppl_code AS char(40))) , 1, 0) as varchar(42)) as puid
, PerViewDataMart.[dbo].[udfIntToGuid] ([ppl_code]) as projectUID
, cast (ppl_code as varchar(50)) as pplCode 
, cast (project_name as varchar(255)) as projectName
, cast ('Planview' as varchar(12) ) as  epm_platform
, cast ([Executive Summary] as varchar(max)) as projectDescription
,[Schedule_Start_Date] as projectStartDate
,[Schedule_Finish_Date] as projectFinishDate
, cast( proj_mgr as varchar(255)) as projectManagerName
, '1/1/1900' as ProjectActualStartDate
, '1/1/1900' as ProjectActualFinishDate
, cast ([percent_complete] as smallint)  as [ProjectPercentCompleted]
, cast( proj_mgr as varchar(255)) as ProjectOwnerName
, 1 as ProjectCount
, 0 as ProjectType
,[Baseline_Start_Date] as ProjectBaseline0StartDate
,[Baseline_Finish_Date] as ProjectBaseline0FinishDate
, cast('unknown' as varchar(4000)) as 'Line of Business'
--, cast (case when project_status = 'Open' then 'In Progress' 
--		when project_status = 'Assumed Completed' then 'Complete' 
--		when project_status = 'On Hold' then 'On Hold' 
--		when project_status = 'Cancelled' then 'Cancelled' 
--		when project_status = 'New' then 'Scheduled'
--		else 'Logged' 
--	end as varchar(40)) as  [Project Active Status]
, cast ('In Progress' as varchar(40)) as [Project Active Status]
, cast ([current status] as varchar(4000)) as 'Overall Status Commentary' 
, cast (ISNULL([BusinessOwner], N'&lt;No Business Owner&gt;') as varchar(40)) as 'Business Owner'
, cast (ISNULL(ProjectSponsor, N'&lt;No Project Sponsor&gt;') as varchar(40)) as 'Project Sponsor'  
--, cast (ProjectSponsor as varchar(40)) as 'Project Sponsor'  
, cast([PM Assessed Project Health] as varchar(12)) as 'PM Assessed Project Health' 
, cast( case when [PM Assessed Project Health] = 'Green' then 1 
	when [PM Assessed Project Health] = 'Yellow' then 2
	when [PM Assessed Project Health] = 'Red' then 3
	else 0
	end as int)  as PMHealth 

, cast (OverDueMilestones as varchar(4000)) as 'Over Due Milestones' 
, cast ([current status] as varchar(4000)) as 'executive status summary' 
, cast ('No' as varchar(4) ) as 'Shell Project Flag'
  FROM [HCAPROD_Copy].[dbo].[dim_mapped_projects]</property>
